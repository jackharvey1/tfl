<html>
<head>
    <script src="/js/utils.js"></script>
    <link rel="stylesheet" href="/styles/main.css" />
</head>
<body>
    {#stations}
        <div class="station" style="top:{top}px;left:{left}px">
            <i class="point" onmouseenter="showTooltip(this)" onmouseleave="hideTooltip(this)"></i>
            <p class="tooltip hidden">{stationName}</p>
        </div>
    {/stations}
</body>
</html>
